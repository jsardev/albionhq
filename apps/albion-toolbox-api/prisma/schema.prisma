generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model AlbionItem {
  id String @id
  tier Int
  weight Int
  max_stack_size Int
  food_category String
  shop_category String
  shop_subcategory String
  resource_type String


}
